{
  "private": true,
  "name": "remix-app-template-js",
  "description": "",
  "license": "",
  "scripts": {
    "build": "remix build",
    "dev": "concurrently -p \"[{name}]\" -n \"Remix,MF\" -c \"bgBlue.bold,bgMagenta.bold\" \"remix watch\" \"npm run start\"",
    "postinstall": "remix setup cloudflare-workers",
    "build:worker": "esbuild --define:process.env.NODE_ENV='\"production\"' --minify --bundle --sourcemap --outdir=dist ./worker",
    "dev:worker": "esbuild --define:process.env.NODE_ENV='\"development\"' --bundle --sourcemap --outdir=dist ./worker",
    "start": "miniflare --build-command \"npm run dev:worker\" --watch",
    "deploy": "npm run build && wrangler publish"
  },
  "dependencies": {
    "@remix-run/cloudflare-workers": "^1.1.3",
    "@remix-run/react": "^1.1.3",
    "@sagi.io/workers-jwt": "^0.0.20",
    "concurrently": "^7.0.0",
    "firebase": "^9.6.4",
    "passport": "^0.5.2",
    "passport-discord": "^0.1.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "remix": "^1.1.3",
    "remix-auth-socials": "^1.0.1"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^2.2.2",
    "@remix-run/dev": "^1.1.3",
    "esbuild": "0.13.14",
    "miniflare": "2.0.0-next.3"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false,
  "main": "dist/worker.js"
}
